{
  "name": "manar-travel-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "google-map-react": "^1.1.4",
    "react": "^16.8.1",
    "react-bootstrap": "^1.0.0-beta.11",
    "react-dom": "^16.8.1",
    "react-google-maps": "^9.4.5",
    "react-notifications": "^1.4.3",
    "react-redux": "^5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "^3.0.0",
    "redux": "^3.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "start": "SET REACT_APP_CUSTOM_ENV=development&& react-scripts start",
    "start-prod": "SET REACT_APP_CUSTOM_ENV=production&& react-scripts start",
    "build": "SET REACT_APP_CUSTOM_ENV=production&&react-scripts build",
    "test": "jest --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.7.1",
    "dotenv": "^8.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.7.1",
    "puppeteer": "^1.19.0",
    "react-test-renderer": "^16.8.6"
  }
}
